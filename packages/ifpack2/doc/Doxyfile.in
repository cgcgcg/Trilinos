#
# This file is used by doxygen for building documentation in the build tree
#
@INCLUDE         = @CMAKE_CURRENT_BINARY_DIR@/../../../doc/Doxyfile
@INCLUDE         = @CMAKE_CURRENT_SOURCE_DIR@/Doxyfile.options

PROJECT_NAME     = @PACKAGE_NAME@
INPUT            = @CMAKE_CURRENT_SOURCE_DIR@/index.doc @CMAKE_CURRENT_SOURCE_DIR@/../src
EXCLUDE          = @CMAKE_CURRENT_SOURCE_DIR@/../test
OUTPUT_DIRECTORY = @LOCAL_DOXYGEN_OUTPUT_DIR@
GENERATE_TAGFILE = @DOXYGEN_TAG_OUTPUT_DIR@/@PACKAGE_NAME@.tag
TAGFILES         = @DOXYGEN_TAG_OUTPUT_DIR@/Teuchos.tag=@Teuchos_DOXYGEN_OUTPUT_DIR@/html \
                   @DOXYGEN_TAG_OUTPUT_DIR@/belos.tag=@Belos_DOXYGEN_OUTPUT_DIR@/html
                   @DOXYGEN_TAG_OUTPUT_DIR@/Anasazi.tag=@Anasazi_DOXYGEN_OUTPUT_DIR@/html
PREDEFINED             = DOXYGEN_SHOULD_SKIP_THIS \
                         HAVE_IFPACK2_EXPERIMENTAL \
                         HAVE_IFPACK2_AMESOS2
